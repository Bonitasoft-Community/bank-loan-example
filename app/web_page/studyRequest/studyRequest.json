      "type" : "expression",
      "value" : [
        "if( $data.loanRequest && $data.loanRequest_type ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.loanRequest.type = $data.loanRequest_type;",
        "\treturn {",
        "\t\t//map loanRequest variable to expected task contract input",
        "\t\tloanRequestInput: {",
